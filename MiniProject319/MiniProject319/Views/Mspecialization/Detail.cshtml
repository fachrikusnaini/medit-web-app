@using MiniProject319.DataModels;
@model MSpecialization

@{
    Layout = null;
    ViewData["Title"] = "Detail Spesialis Dokter";
}



<div class="card">
    <div class="card-body">

        <div class="h4 mb-4">@ViewData["Title"]</div>

        <div class="form-group row">
            <label for="Name" class="col-sm-2 col-form-label">Name</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" id="Name" name="Name" value="@Model.Name" readonly>
               
            </div>
        </div>

        <div class="form-group row mt-3">
            <div class="col-sm-10">
                <a class="btn btn-light" asp-controller="Mspecialization" asp-action="Index"> Back to list </a>
            </div>
        </div>


    </div>
</div>